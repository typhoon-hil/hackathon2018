            new_state = 0;
        else if (duration_expired == 0 && on==0)
            new_state = 3;
        else
            new_state = old_state;
        break;
    case 3 : // ON HOLD
        if(on==1)
            new_state = 2;
        else
            new_state = old_state;
        break;
